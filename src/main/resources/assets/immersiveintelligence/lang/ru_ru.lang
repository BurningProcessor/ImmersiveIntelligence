ie.manual.entry.infiltrators_goggles.name=Очки разведчика
ie.manual.entry.infiltrators_goggles.subtext=Übervision
ie.manual.entry.infiltrators_goggles0=

#Reference to Orwell's 1984
ie.manual.entry.infrared_observer.name=Инфракрасный наблюдатель
ie.manual.entry.infrared_observer.subtext=Большой брат
ie.manual.entry.infrared_observer0=

#Reference to the Battle of Britain in WW2
ie.manual.entry.radar.name=Радар
ie.manual.entry.radar.subtext=Реликвия из Дувра
ie.manual.entry.radar0=

#Reference to a book by Sun Tzu
